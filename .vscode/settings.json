{
    "makefile.launchConfigurations": [
        {
            "cwd": "${workspaceFolder}",
            "binaryPath": "${workspaceFolder}/bin/uxncli.exe",
            "binaryArgs": ["test.rom"]
        },
        {
            "cwd": "${workspaceFolder}",
            "binaryPath": "${workspaceFolder}/bin/uxnemu.exe", 
